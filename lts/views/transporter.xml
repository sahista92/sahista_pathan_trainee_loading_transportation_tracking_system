<odoo>
	<data>
		 <record model="ir.ui.view" id="lts_list_transporter_regi"> 
		      <field name="name">TR list</field>
		      <field name="model">transporter_regi.transporter_regi</field>
		      <field name="arch" type="xml">
		        <tree editable="top">
		          <field name="name"/>
		          <field name="email"/>
		          <field name="c_number"/>
		          <field name="gender"/>
		          <field name="address"/>
		          <field name="city"/>
		          <field name="state"/>
		          <field name="password"/>
		        </tree>
		      </field>
		  </record>
		 <record model="ir.ui.view" id="lts_list_transporter_regi_search_filter"> 
		      <field name="name">TR list</field>
		      <field name="model">transporter_regi.transporter_regi</field>
		      <field name="arch" type="xml">
				      <search string="City Search">
		                <filter name="City" string="Viramgam" domain="[('city', '=', 'vgm')]"/>
		                <field name="city" filter_domain="[('city','not ilike',self),('district','not ilike',self)]"/>
		              </search>
		      </field>
		  </record>				  


		<record model="ir.actions.act_window" id="lts_action_window_transporter_regi">
	      <field name="name">Transporter_Regi</field>
	      <field name="res_model">transporter_regi.transporter_regi</field>
	      <field name="view_mode">tree,form,graph</field>
	    </record>



	        <menuitem name="Transporter_Regis" id="lts_menu_1_tranporter_regi" parent = "lts_menu_root" action="lts_action_window_transporter_regi"/>
	</data>
</odoo>